<core:FragmentDefinition xmlns="sap.m"
	xmlns:core="sap.ui.core" xmlns:f="sap.ui.layout.form">
	<Dialog xmlns="sap.m" id="idNewApp" icon=""
		showHeader="true" type="Standard">
		<f:SimpleForm id="SimpleFormDisplay354" maxContainerCols="2"
			editable="false" layout="ResponsiveGridLayout" labelSpanL="3"
			labelSpanM="3" emptySpanL="4" emptySpanM="4" columnsL="1" columnsM="1">
			<f:content>
				<Label xmlns="sap.m" id="idDescriptionLabelApp"/>
				<Input xmlns="sap.m" id="idConfigDescription"
					editable="true" liveChange ="applicationDescChange"/>
				<Label xmlns="sap.m" id="idSemanticObjectLabelApp"/>
				<Input xmlns="sap.m" id="idConfigSemantic"
					editable="true"/>
			</f:content>
		</f:SimpleForm>
		<beginButton>
			<Button xmlns="sap.m" id="idSaveButtonApp" type="Default" width=""
				enabled="false" visible="true" press="handleSaveDialogPress">
			</Button>
		</beginButton>
		<endButton>
			<Button xmlns="sap.m" id="idCancelButtonApp" type="Default"
				width="" enabled="true" visible="true" 	press="closeNewAppDialog">
			</Button>
		</endButton>
	</Dialog>
</core:FragmentDefinition>