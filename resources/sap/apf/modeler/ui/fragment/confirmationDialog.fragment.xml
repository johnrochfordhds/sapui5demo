<core:FragmentDefinition xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<Dialog xmlns="sap.m" id="idDeleteConfirmation"
		showHeader="true" type="Standard">
		<beginButton>
			<Button xmlns="sap.m" id="idDeleteButton" type="Default"
				enabled="true" visible="true" iconFirst="true"
				iconDensityAware="true" press="handleConfirmDeletion">
			</Button>
		</beginButton>
		<endButton>
			<Button xmlns="sap.m" id="idCancelButtonDialog" type="Default"
				enabled="true" visible="true" iconFirst="true"
				iconDensityAware="true" press="closeDialog">
			</Button>
		</endButton>
	</Dialog>
</core:FragmentDefinition>